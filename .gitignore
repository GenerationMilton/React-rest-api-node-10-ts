# Node.js / Express server .gitignore

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage reports
coverage/
*.lcov

# Dependency directories
node_modules/

# Build output
dist/
build/

# TypeScript cache
*.tsbuildinfo

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# OS files
.DS_Store
Thumbs.db

# IDE/editor settings
.vscode/
.idea/
*.swp

# Optional: if using Docker
docker-compose.override.yml
.docker/

# Optional: if using PM2
.pm2/

# Optional: if using Prisma or other ORMs
prisma/migrations/

# Optional: if using Next.js or similar frameworks in monorepo
.next/
out/
